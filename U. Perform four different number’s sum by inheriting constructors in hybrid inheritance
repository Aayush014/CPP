#include <iostream>
using namespace std;

class A
{
 	public:
 		int a;
};
class B : public A
{
 	public:
 		int b;
};
class C
 {
 	public:
 		int c;
};
class D : public B, public C 
{
 	public:
 		void sum()
 	{
 		cout << "Enter A:- ";
 		cin >> a;
 		cout << "Enter B:- ";
 		cin >> b;
 		cout << "Enter C:- ";
 		cin >> c;
 		
 	  cout << "Sum= " << a + b +c;
 	}
};

int main()
{
  D obj1;       
 	obj1.sum();
 	return 0;
}
